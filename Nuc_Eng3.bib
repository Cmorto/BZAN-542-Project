@article{ WOS:000730809000006,
Author = {Johnson, Blake A. and Di Iorio, John R. and Roman-Leshkov, Yuriy},
Title = {Tailoring Distinct Reactive Environments in Lewis Acid Zeolites for
   Liquid Phase Catalysis},
