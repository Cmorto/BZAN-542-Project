@article{ WOS:000447170000009,
Author = {Cornejo, Christian E. and Bertram, Michelle E. and Diaz, Timoteo C. and
   Narayan, Saaketh R. and Ram, Sukesh and Kavanagh, Karen L. and Herbots,
   Nicole and Day, Jack M. and Ark, Franscesca J. and Dhamdhere, Ajit and
   Culbertson, Robert J. and Islam, Rafiqul},
Title = {Measuring Surface Energies of GaAs (100) and Si (100) by Three Liquid
   Contact Angle Analysis (3LCAA) for Heterogeneous Nano-Bonding (TM)},
Journal = {MRS ADVANCES},
Year = {2018},
Volume = {3},
Number = {57-58},
Pages = {3403-3411},
Abstract = {Analysis of the total surface energy gamma(T) and its three components
   as established by the van Oss-Chaudhury-Good Theory (vOCG) is conducted
   via Three Liquid Contact Angle Analysis (3LCAA). gamma(T) is correlated
   with the composition of the top monolayers (ML) obtained from
   High-Resolution Ion Beam Analysis (HR-IBA). Control of gamma(T) enables
   surface engineering for wafer bonding (Nano-Bonding (TM)) and/or
   epitaxial growth. Native oxides on boron-doped pSi(100) are found to
   average gamma(T) of 53 +/- 1.4 ml/m(2)) and are always hydrophilic. An
   I-IF in methanol or aqueous I-IF etch for 60 s always renders 51(100)
   hydrophobic. Its gamma(T) decreases by 20\% to 44 +/- 3 ml/m(2)) in I-IF
   in methanol etch and by 10\% to 48 +/- 3 ml/m(2) in aqueous HF. On the
   contrary, GaAs(100) native oxides are found to always be hydrophobic.
   Tellurium n+doped GaAs(100) yields an average of gamma(T) of 37 +/- 2
   ml/ml , 96\% of which is due to the LifshitzVan der Waals molecular
   interactions (gamma(LW) = 36 +/- 1 ml/m(2)). However, hydrophobic
   GaAs(100) can be made highly hydrophilic. After etching, gamma(T)
   increases by almost 50\% to 66 +/- 1.4 ml/m(2). 3LCAA shows that the
   gamma(T) increase is due to electron acceptor and donor interactions,
   while the Lifshitz-van der Wools energy gamma(LW) remains constant. IBA
   combining the 3.039 +/- 0.01 MeV oxygen nuclear resonance with < 111 >
   channeling, shows that oxygen on 51(100) decreases by 10\% after aqueous
   HF etching, from 13.3 +/- 0.3 monolayers (ML) to 11.8 +/- 0.4 ML 1 hour
   after etch.Te-doped GaAs(100) exhibits consistent oxygen coverage of 7.2
   +/- 1.4 ML, decreasing by 50\% after etching to a highly hydrophilic
   surface with 3.6 +/- 0.2 oxygen ML. IBA shows that etching does not
   modify the GaAs surface stoichiometry to within 1\%. Combining 3LCAA
   with HR-IBA provides a quantitative metrology to measure how GaAs and Si
   surfaces can be altered to a different hydroaffinity and surface
   termination.},
