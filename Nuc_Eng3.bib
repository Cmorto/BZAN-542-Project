@article{ WOS:000505255800010,
Author = {Wang, Haijing and Stockhausen, Ed and Wyatt, Jr., Dennis F. and Gulick,
   Dean},
Title = {Modeling of Azimuthal Gamma-Ray Tools for Use in Geosteering in
   Unconventional Reservoirs},
