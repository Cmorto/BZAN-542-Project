Author = {Gohil, Shalini V. and Padmanabhan, Aiswaria and Kan, Ho-Man and Khanal,
   Manakamana and Nair, Lakshmi S.},
Title = {Degradation-Dependent Protein Release from Enzyme Sensitive Injectable
   Glycol Chitosan Hydrogel},
