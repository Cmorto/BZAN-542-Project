@article{ WOS:000649108800007,
Author = {Ifft, Edward M. and Koplow, David A.},
Title = {Legal and political myths of the Treaty on the Prohibition of Nuclear
   Weapons},
Journal = {BULLETIN OF THE ATOMIC SCIENTISTS},
Year = {2021},
Volume = {77},
Number = {3, SI},
Pages = {134-139},
Month = {MAY 4},
Abstract = {The Treaty on the Prohibition of Nuclear Weapons is an important
   diplomatic, political, and legal landmark in the longstanding effort to
   abolish nuclear weapons. It has already been joined by 54 countries, but
   has been rejected by all the states that currently possess nuclear
   weapons. Accurate and even-handed analysis of the treaty's potential
   contributions, its relationship to other legally-binding instruments,
   and its likely future have been obscured by the perpetuation of a series
   of legal and political myths, which this article seeks to expose and
   clarify.},
