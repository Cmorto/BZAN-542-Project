@article{ WOS:000861747900003,
Author = {Bolotnikov, A. and Carini, G. and Chekhlov, M. and Dellapenna, A. and
   Fried, J. and Haupt, J. and Herrmann, S. and Kotov, I and Medvedev, D.
   and Moiseev, A. and Pinaroli, G. and Rusek, A. and Sasaki, M. and
   Sivertz, M. and Smith, L. and Yates, E.},
Title = {Radiation effects induced by the energetic protons in 8x8x32 mm3 CdZnTe
   detectors},
Journal = {NUCLEAR INSTRUMENTS \& METHODS IN PHYSICS RESEARCH SECTION
   A-ACCELERATORS SPECTROMETERS DETECTORS AND ASSOCIATED EQUIPMENT},
Year = {2022},
Volume = {1039},
Month = {SEP 11},
Abstract = {CdZnTe (CZT) detectors have been used in the past and are being actively
   considered for future gamma-ray space telescopes. One of the challenges
   of operating CZT detectors in low Earth orbit is radiation damage caused
   by energetic protons. Previous studies concluded that fluences of 10(8)
   -10(9) p/cm(2) are sufficient to cause shifting of peak positions and
   degradations of energy resolution. Degradation of detectors after proton
   irradiation has been extensively investigated to determine their
   radiation resistance limits and performance recovering procedures for
   their applications in space and nuclear nonproliferation. Here we
   present the study of radiation effects induced by 100 MeV protons in
   3-cm long CdZnTe detectors that we recently proposed for the gamma-ray
   telescope GECCO. The goal of this study is to evaluate the feasibility
   of using such long CZT detectors in future space missions.},
Publisher = {ELSEVIER},
Address = {RADARWEG 29, 1043 NX AMSTERDAM, NETHERLANDS},
Type = {Article},
