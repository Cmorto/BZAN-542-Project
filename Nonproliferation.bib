@article{ WOS:000685514500010,
Author = {Foley, Ari and Yang, Haori},
Title = {Short-lived photofission product yields from U-238 and Th-232 at
   Bremsstrahlung X-ray endpoint energies of 8, 14, and 20 MeV for nuclear
   forensics isotope production applications},
Journal = {NUCLEAR INSTRUMENTS \& METHODS IN PHYSICS RESEARCH SECTION
   A-ACCELERATORS SPECTROMETERS DETECTORS AND ASSOCIATED EQUIPMENT},
Year = {2021},
Volume = {1013},
Month = {OCT 11},
Abstract = {The existing photofission product yield data sets are often based on
   nuclear models, and limited experimental measurements of these fission
   product yields have been performed. These experiments resulted in
   measured cumulative fission product yields (CFPYs) for U-238 and Th-232
   for the bremsstrahlung X-ray endpoint energies of 8, 14, and 20 MeV. The
   half-lives of the reported fission products range from 1.07-40.8 s. This
   work is motivated by a demonstrated need for novel isotope production
   methods for nuclear forensics and improved nuclear data to support the
   nonproliferation community. A high-purity germanium detector and a
   pneumatic transfer system were employed for measurements of short-lived
   fission products, collecting data between cycles of accelerator
   irradiation and measurement.},
