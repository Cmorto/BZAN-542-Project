@article{ WOS:000511297400064,
Author = {Liang, Y. and Wen, X. and Baciak, J. E. and Enqvist, A.},
Title = {Measurement of neutron and gamma-ray cross-correlation functions with
   He-4 detectors},
Journal = {NUCLEAR INSTRUMENTS \& METHODS IN PHYSICS RESEARCH SECTION
   A-ACCELERATORS SPECTROMETERS DETECTORS AND ASSOCIATED EQUIPMENT},
Year = {2020},
Volume = {954},
Month = {FEB 21},
Note = {17th Symposium on Radiation Measurements and Applications (SORMA), Univ
   Michigan, Ann Arbor, MI, JUN 11-14, 2018},
Abstract = {For the first time, the neutron and gamma-ray time-dependent
   cross-correlation distributions from spontaneous fission and (alpha, n)
   neutron sources are measured with the He-4 fast neutron detectors. The
   pulse shape discrimination technique based on charge comparison is
   utilized to discriminate between neutrons and gamma-rays in the
   identification of the four possible correlated pairs: (gamma, gamma),
   (n, n), (gamma, n), and (n, n). Both total and individual
   cross-correlation functions are obtained, and their shapes can be easily
   distinguished among different neutron sources. The (n, n) and (gamma, n)
   correlations are selectively analyzed and the peak position of the (n,
   n) and (gamma, n) pairs shows a linear correlation with source-detector
   distance. The measurements illustrate the feasibility of using He-4
   detectors to identify nuclear materials and their geometrical
   configurations, as well as other applications in nuclear
   nonproliferation and safeguards. In particular, the (n, n) correlated
   data can be very clearly assayed even in presence of significant gamma
   fields without the use of shielding.},
