@article{ WOS:000488777700001,
Author = {Hua, Michael Y. and Goddard, Braden and Lloyd, Cody and Leppink, Evan C.
   and Abraham, Sara A. and Noey, Jordan D. and Clarke, Shaun D. and Pozzi,
   Sara A.},
Title = {Simulation of the Nondestructive Assay of Np-237 Using Active Neutron
   Multiplicity Counting},
Journal = {NUCLEAR SCIENCE AND ENGINEERING},
Year = {2020},
Volume = {194},
Number = {2},
Pages = {154-162},
Month = {FEB 1},
Abstract = {In this work, an epithermal neutron multiplicity counter (ENMC) and an
   organic scintillator multiplicity counter (OSMC) are compared in the
   assay of Np-237, a potentially weapons-usable isotope, using active
   neutron multiplicity counting. In active neutron multiplicity counting,
   the neutron doubles and triples multiplicity rates are used to
   analytically calculate sample mass. To distinguish the masses of two
   different samples, the measured triples and doubles rates of each sample
   must be separated by 1 sigma. The time it takes each system to separate
   the measured triples multiplicity rates was compared using 20 metal
   samples of Np-237 with masses logarithmically distributed between 10 and
   1000 g. The results show that the OSMC can distinguish the smallest
   masses (10.0 and 12.7 g) beyond 1 sigma in 20 min and that the ENMC
   requires more than 400 times the measurement time to obtain the same
   precision. Similarly, the OSMC is at least 4.5 times faster than the
   ENMC in separating the doubles multiplicity rates.},
Publisher = {TAYLOR \& FRANCIS INC},
Address = {530 WALNUT STREET, STE 850, PHILADELPHIA, PA 19106 USA},
