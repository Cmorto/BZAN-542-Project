@article{ WOS:000865422400002,
Author = {Li, Viacheslav A. and Dazeley, Steven A. and Bergevin, Marc and
   Bernstein, Adam},
Title = {Scalability of Gadolinium-Doped-Water Cherenkov Detectors for Nuclear
   Nonproliferation},
