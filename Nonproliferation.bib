@article{ WOS:000811767100001,
Author = {Laplace, Thibault A. and Goldblu, Bethany L. and Brown, Joshua A. and
   LeBlanc, Glenn and Li, Tianyue and Manfredi, Juan J. and Brubaker, Erik},
Title = {Modeling ionization quenching in organic scintillators},
Journal = {MATERIALS ADVANCES},
Year = {2022},
