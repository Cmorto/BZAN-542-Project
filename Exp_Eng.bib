@article{ WOS:000525398800008,
Author = {Qian, Rongyi and Liu, Lanbo},
Title = {Imaging the active faults with ambient noise passive seismics and its
   application to characterize the Huangzhuang-Gaoliying fault in Beijing
   Area, northern China},
