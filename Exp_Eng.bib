@article{ WOS:000506655300001,
Author = {Karothu, Durga Prasad and Halabi, Jad Mahmoud and Li, Liang and
   Colin-Molina, Abraham and Rodriguez-Molina, Braulio and Naumov, Pance},
Title = {Global Performance Indices for Dynamic Crystals as Organic Thermal
   Actuators},
