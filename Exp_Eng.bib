@article{ WOS:000619073800001,
Author = {Lee, Kibaek and Hernandez, Alberto M. and Stewart, D. Scott and Lee,
   Seungjoon},
Title = {Data-driven blended equations of state for condensed-phase explosives},
Journal = {COMBUSTION THEORY AND MODELLING},
Year = {2021},
Volume = {25},
Number = {3},
Pages = {413-435},
Month = {APR 16},
Abstract = {We present a data-driven blended equation of state (EOS) approach for
   condensed phase high explosive materials. We first calibrate four
   different high explosive materials (Nitromethane, HMX, PETN and TATB)
   using a single or blending multiple Fried Howard Gibbs (FHG) EOS by an
   ad hoc trial and error method that has been used in the past, and which
   leads to a predictive model that can be used in engineering
   calculations. This ad-hoc calibration is then re-calibrated based on
   Bayesian optimisation via Gaussian Process regression. The two
   calibrations are then compared qualitatively and quantitatively and are
   shown to be in good to excellent agreement.},
