@article{ WOS:000571252900007,
Author = {Wang, Zhongrui and Li, Can and Lin, Peng and Rao, Mingyi and Nie,
   Yongyang and Song, Wenhao and Qiu, Qinru and Li, Yunning and Yan, Peng
   and Strachan, John Paul and Ge, Ning and McDonald, Nathan and Wu, Qing
