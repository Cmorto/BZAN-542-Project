@article{ WOS:000475838100008,
Author = {Li, Rui and Wang, Jinhu and He, Yi and Dong, Faqin and Bian, Liang and
   Li, Bowen},
Title = {Mechanochemical Synthesis of Defective Molybdenum Trioxide, Titanium
   Dioxide, and Zinc Oxide at Room Temperature},
