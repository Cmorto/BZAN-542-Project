@article{ WOS:000450820400007,
Author = {Wang, Zegao and Wu, Hong-Hui and Li, Qiang and Besenbacher, Flemming and
   Zeng, Xiao Cheng and Dong, Mingdong},
Title = {Self-scrolling MoS2 metallic wires},
Journal = {NANOSCALE},
Year = {2018},
Volume = {10},
