@article{ WOS:000579335100001,
Author = {Rae, P. J. and Dickson, P. M.},
Title = {Some Observations About the Drop-weight Explosive Sensitivity Test},
Journal = {JOURNAL OF DYNAMIC BEHAVIOR OF MATERIALS},
